    &lt;script&gt;
      'use strict';
      document.cookie = "MFI=##purl##;domain=oc.edu;expires=Fri, 31 Dec 9999 23:59:59 GMT; path=/";
      var formResp = [];
      var actionBtn;

      function _submit() {
        $('input[name=submitaction]').val(actionBtn.dataset.action);
        $('input[name=form_resp]').val(formResp.toString());

        if(actionBtn.id == "visit") {
          window.open("https://oc.edu/admissions/undergraduate/visit/?purl=##purl##");
        }
        
        return true;
      }
      
      $(document).ready(function() {
        $('#ready, #visit').click(function() {
          actionBtn = this;
        });

        $('.icon').css('opacity', 1);

        $('.max-width-768 tr').click(function() {
          var data = this.dataset.resp;
          var icon = $(this).find('.icon')[0];
          
          if(icon.style.opacity == 0) {
            icon.style.opacity = 1;
          } else {
            icon.style.opacity = 0;
          }

          for(var i = 0; i &lt; formResp.length; i++) {
            if(data == formResp[i]) {
              formResp.splice(i,1);
              return;
            }
          }

          formResp.push(data);
        });
      });
